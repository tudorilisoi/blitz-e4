{
  "name": "blitz-e4",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "cross-env NODE_ENV=production blitz build -e production",
    "dev": "cross-env NODE_ENV=development blitz dev",
    "docker": "sh -c 'docker/run-compose.sh $0'",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "prepare": "husky install",
    "browserslist": "npx --yes browserslist@latest"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
