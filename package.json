{
  "name": "blitz-e4",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf ./dist",
    "_dev": "blitz dev",
    "_build": "blitz build",
    "build:1-blitz": "cross-env NODE_ENV=production blitz build",
    "build:2-ws-trpc": "cross-env NODE_ENV=production PORT=3001 rm -rf ./dist; npx tsc --project tsconfig.server.json",
    "build:3-ws-alias": "tsc-alias -p tsconfig.server.json",
    "build": "run-s build:*",
    "dev:1-blitz": "blitz dev",
    "dev:2-ws": "cross-env PORT=3001 tsx watch src/ws-server/wsServer.ts --tsconfig tsconfig.server.json ",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "devx": "blitz dev  -H 192.168.22.100",
    "docker": "sh -c 'docker/run-compose.sh $0'",
    "start:1-blitz": "blitz start",
    "start:2-ws": "cross-env NODE_ENV=production; node dist/src/ws-server/wsServer.js ",
    "start": "cross-env NODE_ENV=production run-p start:*",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@blitzjs/auth": "2.0.5",
